{
	"dashboard": {
		"id": null,
		"title": "Redis Operations Dashboard",
		"tags": ["redis", "performance", "cybership"],
		"style": "dark",
		"timezone": "browser",
		"editable": true,
		"hideControls": false,
		"graphTooltip": 1,
		"time": {
			"from": "now-1h",
			"to": "now"
		},
		"timepicker": {
			"refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
			"time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
		},
		"refresh": "30s",
		"schemaVersion": 30,
		"version": 1,
		"panels": [
			{
				"id": 1,
				"title": "Redis Operations - Average Duration by Operation",
				"type": "stat",
				"targets": [
					{
						"expr": "rate(redis_operation_duration_seconds_sum[5m]) / rate(redis_operation_duration_seconds_count[5m])",
						"format": "time_series",
						"legendFormat": "{{operation}}",
						"refId": "A"
					}
				],
				"fieldConfig": {
					"defaults": {
						"color": {
							"mode": "palette-classic"
						},
						"custom": {
							"displayMode": "list",
							"orientation": "horizontal"
						},
						"mappings": [],
						"thresholds": {
							"steps": [
								{
									"color": "green",
									"value": null
								},
								{
									"color": "yellow",
									"value": 0.01
								},
								{
									"color": "red",
									"value": 0.1
								}
							]
						},
						"unit": "s"
					}
				},
				"options": {
					"reduceOptions": {
						"values": false,
						"calcs": ["lastNotNull"],
						"fields": ""
					},
					"orientation": "auto",
					"text": {},
					"textMode": "auto",
					"colorMode": "value",
					"graphMode": "area",
					"justifyMode": "auto"
				},
				"pluginVersion": "8.0.0",
				"gridPos": {
					"h": 8,
					"w": 12,
					"x": 0,
					"y": 0
				}
			},
